<actuator>
    <!-- All actuator definitions from MyoHand, rewritten with valid attributes -->
    
    <general name="ECRL" class="muscle" tendon="ECRL_tendon"
             forcerange="0 337.3"
             lengthrange="0.313191 0.341072"/>
             
    <general name="ECRB" class="muscle" tendon="ECRB_tendon"
             forcerange="0 252.2"
             lengthrange="0.26997 0.301495"/>
    
    <!-- ... repeat for all other muscles ... -->
    
    <general name="UI_UB5" class="muscle" tendon="UI_UB5_tendon"
             forcerange="0 47.9"
